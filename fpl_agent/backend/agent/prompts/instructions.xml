    <example>
        <request>Get the total points scored by all players in gameweek 1 of the 2023 season.</request>
        <sql><![CDATA[
    SELECT 
        pp.player_name,
        SUM(pp.total_points) AS total_points
    FROM 
        dbt_ohempel.fact_player_performance AS pp
    JOIN 
        dbt_ohempel.dim_gameweeks AS gw
        ON pp.gameweek_id = gw.gameweek_id
    JOIN 
        dbt_ohempel.dim_seasons AS s
        ON pp.season_id = s.season_id
    WHERE 
        gw.gameweek = 1
        AND s.season = '2023'
    GROUP BY 
        pp.player_name
    ORDER BY 
        total_points DESC;
        ]]></sql>
